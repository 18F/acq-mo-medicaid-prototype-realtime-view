{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass src/styles/main.scss --output dist/app.css",
    "build-js": "browserify -d -t [ babelify --presets [ react es2015 ] ] src/app.js -o dist/app.js",
    "build": "npm run build-js",
    "watch-css": "node-sass -w src/styles/ -o dist/",
    "watch-js": "watchify -d -t [ babelify --presets [ react es2015 ] ] src/app.js -v -o dist/app.js",
    "watch": "npm run watch-css & npm run watch-js && fg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "frisbee": "^1.1.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "updeep": "^1.0.0",
    "uswds": "^1.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "node-sass": "^4.5.0",
    "watchify": "2.6.0"
  }
}
